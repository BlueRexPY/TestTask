{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./app",
    "paths": {
      "@/components/*":["components/*"],
      "@/layout/*":["layout/*"],
      "@/assets/*":["assets/*"],
      "@/store/*":["store/*"],
      "@/hooks/*":["hooks/*"],
      "@/utils/*":["utils/*"],
      "@/api/*":["api/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/tracks/createtsx", "app/tests/utils/utils.test.ts"],
  "exclude": ["node_modules"]
}
